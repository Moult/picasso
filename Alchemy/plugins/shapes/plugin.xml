<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">

<plugin id="alchemy.create.shapes" version="1.0.3">
    
    <requires> 
        <import plugin-id="alchemy.core"/> 
    </requires> 
    
    <runtime>
        <library id="shapes" path="/" type="code">
            <export prefix="*" /> 
        </library>
        <library type="resources" path="data/" id="data" />
    </runtime>
    
    <extension plugin-id="alchemy.core" point-id="Create" id="shapesCreate"> 
        <parameter id="class" value="alchemy.create.Shapes"/> 
        <parameter id="name" value="Shapes"/> 
        <parameter id="description" value="Create standard shapes" />
        <parameter id="icon" value="tab.gif" />
    </extension>
    
</plugin>