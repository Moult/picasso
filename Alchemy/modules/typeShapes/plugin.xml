<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">

<plugin id="alchemy.create.typeShapes" version="1.0.3">
    
    <requires> 
        <import plugin-id="alchemy.core"/> 
    </requires> 
    
    <runtime>
        <library id="typeShapes" path="/" type="code">
            <export prefix="*" /> 
        </library>
        <library type="resources" path="data/" id="data" />
    </runtime>
    
    <extension plugin-id="alchemy.core" point-id="Create" id="typeShapesCreate"> 
        <parameter id="class" value="alchemy.create.TypeShapes"/> 
        <parameter id="name" value="Type Shapes"/> 
        <parameter id="description" value="Shapes generated from type characters" />
        <parameter id="icon" value="icon.png" />
    </extension>
    
</plugin>